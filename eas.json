{
  "cli": {
    "version": ">= 3.10.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "BASE_URL": "https://backend-bodega-alphagroup.onrender.com/api"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "BASE_URL": "https://backend-bodega-alphagroup.onrender.com/api"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
